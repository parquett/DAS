@{
    Layout = "~/Views/Master/_Ajax.cshtml";
}
<table border="0" cellpadding="0" cellspacing="0" class="printTable">
    <tbody>
        <tr>
            <th>
                <img height="30px" src="{KFA_logo}" />
                <h3>
                    Agencija za Hranu i Veterinartsvo
                    Food and Veterinary Agency
                </h3>
            </th>
            <th style="background-color: #D9D9D9">
                <h3>
                    Sample Docuemnt<br />
                    Departament {SectionName}
                </h3>
            </th>
            <th style="background-color: #D9D9D9">
                <h3>
                    FSC 5.10-1
                    Ver. 2.0
                </h3>
            </th>
        </tr>
        <tr>
            <td>&nbsp;</td>
            <td style="background-color: #D9D9D9">
                <h3>COD: FPO-10-01</h3>
            </td>
            <td style="background-color: #D9D9D9">
                <h3>Faqe 1 nga 1</h3>
            </td>
        </tr>
    </tbody>
</table>

<div class="resultsreport-info">
    Numri Serik:<br />
    Serijski broj/ Serial number:
    <div class="report-parameter">{UniqueCod}</div>
    .<br />
    Data e pranimit të mostrës(ave):<br />
    Datum prijema uzorka/Date of sample reception
    <div class="report-parameter">{DateSection}</div>
    .<br />
    Sample Name
    <div class="report-parameter">{SampleName}</div>
    .<br />
    Sample Qty
    <div class="report-parameter">{Quantity}</div>
    .<br />
    Sampling Reason
    <div class="report-parameter">{Gosts}</div>
</div>
{RequestTable}

<div class="sing-place">
    <div class="sing-container">
        <div class="sing-text" style="text-transform: none">Ora <span class="report-parameter">{TimeSection}</span></div>

        <div class="sing-text">&nbsp;&nbsp;&nbsp;&nbsp;</div>

        <div class="sing-text" style="text-transform:none">Signature</div>

        <div class="sing-placeholder">________________</div>

        <div class="sing-text">&nbsp;</div>
    </div>
</div>

<div class="dash-separator">&nbsp;</div>
{ResultsTable}